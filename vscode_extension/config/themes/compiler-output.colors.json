{
  "$schema": "vscode://schemas/color-theme",
  "name": "Delphi Compiler Colors",
  "type": "dark",
  "tokenColors": [
    {
      "scope": "markup.error.delphi",
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "scope": "markup.warning.delphi",
      "settings": {
        "foreground": "#fdcb6e"
      }
    },
    {
      "scope": "markup.hint.delphi",
      "settings": {
        "foreground": "#74b9ff"
      }
    },
    {
      "scope": "markup.info.delphi",
      "settings": {
        "foreground": "#55a3ff"
      }
    },
    {
      "scope": "constant.other.error-code.delphi",
      "settings": {
        "foreground": "#a29bfe"
      }
    },
    {
      "scope": "string.other.filepath.delphi",
      "settings": {
        "foreground": "#00b894"
      }
    },
    {
      "scope": "constant.numeric.line-number.delphi",
      "settings": {
        "foreground": "#ffeaa7"
      }
    }
  ]
}
